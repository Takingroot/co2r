html
  font-family arial

body, html
  padding 0
  margin 0

a:link
  text-decoration none

*
  box-sizing border-box

.vintage-box-models *
  box-sizing content-box

.unspaced
  margin-top 0
  margin-bottom 0

ul.unstyled
  margin-left 0
  margin-bottom 0

.carousel.slide
  overflow hidden

.ui-match
  highlight-color = hsla(193, 90%, 61%, 0.09)
  background highlight-color
  outline 4px solid highlight-color
  // "dark" background effect
  //background hsla(60, 100%, 01%, 0.15)
  color hsl(79, 70%, 61%)
  //border-bottom 1px dashed hsla(0, 0, 100, 20%)
